{
  "collectCoverageFrom": [
    "app/**/*.{js,jsx}",
    "!src/**/*.test.{js,jsx}",
    "!src/**/*.stories.{js,jsx}",
    "!src/stories/**/*.{js,jsx}",
    "!src/index.{js,jsx}"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 98,
      "branches": 91,
      "functions": 98,
      "lines": 98
    }
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
