{
  "name": "@laposte-okapi/protagonist-react",
  "version": "0.2.0",
  "main": "lib/index.js",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*.stories.{js,jsx}",
      "!src/stories/**/*.{js,jsx}",
      "!src/index.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    }
  },
  "files": [
    "LICENSE.md",
    "README.md",
    "lib"
  ],
  "keywords": [
    "react",
    "protagonist",
    "apiblueprint",
    "apib"
  ],
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "dompurify": "2.0.7",
    "font-awesome": "4.7.0",
    "highlight.js": "9.10.0",
    "marked": "0.3.6",
    "prop-types": "15.5.10",
    "react-dom-factories": "1.0.1",
    "react-scroll": "1.5.4",
    "reactstrap": "4.8.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-transition-group": "^1.1.3"
  },
  "devDependencies": {
    "@kadira/storybook": "2.21.0",
    "babel-cli": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.22.0",
    "enzyme": "2.8.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "react": "15.6.1",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-scripts": "0.9.5",
    "react-transition-group": "1.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*.stories.js --collectCoverageFrom=!src/stories/**/*.js",
    "eject": "react-scripts eject",
    "prebuild": "BABEL_ENV=production rm -rf lib && babel src/components --out-dir lib --ignore test.js,stories.js --copy-files",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  }
}
